<div class="wrapper">
  <h1>New Download for <%= h @project.name -%></h1>
  <% form_for( [@project, @download] ) do |f| %>
    <%= render :partial => 'form', :object => f %>
    <div class="form_button">
      <%= f.submit "Create" %>
    </div>
  <% end %>
  <div class="bottom_links">
    <%= link_to 'Back', project_downloads_path(:project_id => @project) %>
  </div>
</div>
