<h1>New issue_tracker</h1>

<% form_for(@issue_tracker) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :project_id %><br />
    <%= f.text_field :project_id %>
  </p>
  <p>
    <%= f.label :active %><br />
    <%= f.check_box :active %>
  </p>
  <p>
    <%= f.label :use_internal %><br />
    <%= f.check_box :use_internal %>
  </p>
  <p>
    <%= f.label :external_url %><br />
    <%= f.text_field :external_url %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', issue_trackers_path %>
