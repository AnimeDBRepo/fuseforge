<div class="wrapper">
  <h1>Show Project Category</h1>
  <div class="one_column section">
    <div class="interior">
      <p>
        <b>Name:</b>
        <%=h @project_category.name %>
      </p>
      <p>
        <b>Description:</b>
        <%=h @project_category.description %>
      </p>
      <p>
        <b>Position:</b>
        <%=h @project_category.position %>
      </p>
    </div>
  </div>
  <div class="bottom_links">
    <%= link_to 'Edit', edit_project_category_path(@project_category) %> |
    <%= link_to 'Back', project_categories_path %>
  </div>
</div>