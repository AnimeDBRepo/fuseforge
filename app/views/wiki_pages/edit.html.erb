<h1>Editing Wiki Page for <%= h @project.name -%></h1>

<% form_for( [@project, @wiki_page] ) do |f| %>
  <%= render :partial => 'form', :object => f %>
  Attachments: <%= @wiki_page.attachments.collect { |a| a.attached_file_file_name }.join(', ') %>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', project_wiki_page_path(:project_id => @project, :id => @wiki_page) %> |
<%= link_to 'Back', project_wiki_pages_path(@project) %>
